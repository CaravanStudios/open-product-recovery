{
  "name": "opr-models",
  "version": "1.0.0",
  "description": "Datamodels and schemas for OPR",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm run gentypes && tsc -p .",
    "test": "mocha -r ts-node/register -R spec 'test/**/*.spec.ts'",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile && npm run lint",
    "compileschema": "node --nolazy -r ts-node/register build-tools/compileschema.ts",
    "gentypes": "npm run compileschema -- --src='src/**/*.schema.json' -o src/types-generated",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "sso://foodforgood/opr-unified"
  },
  "author": "Google Food For Good <foodforgood-support-external@google.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "jsonschema": "^1.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.11.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "commander": "^9.4.0",
    "glob": "^8.0.3",
    "glob-promise": "^5.0.0",
    "gts": "^3.1.1",
    "json-refs": "^3.0.15",
    "json-schema-to-typescript": "^11.0.2",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
