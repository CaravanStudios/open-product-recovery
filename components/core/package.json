    "prepare": "npm run compile",
    "pretest": "npm run compile && npm run lint",
    "oprreq": "node --nolazy -r ts-node/register src/bin/oprreq.ts",
    "generatekeypair": "node --nolazy -r ts-node/register src/bin/generatekeypair.ts"
